<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"  namespace="InventoryApplication.Models.Entity" assembly="InventoryApplication">
	<class name="Product" table="Product" >
		<id name="Id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="Name">
			<column name="name" sql-type="varchar(200)" not-null="true"></column>
		</property>
		<property name="Quantity">
			<column name="quantity" sql-type="int" not-null="true" />
		</property>
		<property name="Weight">
			<column name="weight" sql-type="double" not-null="true" />
		</property>
	</class>
</hibernate-mapping>




									