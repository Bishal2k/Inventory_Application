<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"  namespace="InventoryApplication.Models.Entity" assembly="InventoryApplication.Core">
	<class name="Profile" table="profile">
		<id name="Id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="Name">
			<column name="name" sql-type="varchar(200)" not-null="true"></column>
		</property>
		<property name="Contact">
			<column name="contact" sql-type="varchar(10)" not-null="true" />
		</property>
		<property name="Address">
			<column name="address" sql-type="varchar(200)" not-null="true" />
		</property>
		<property name="CompanyName">
			<column name="company_name" sql-type="varchar(20)" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
